{
  "name": "ez-file-upload",
  "displayName": "EZ File Upload",
  "description": "Easily upload files to workspace folders via Explorer context menu",
  "version": "0.2.2",
  "publisher": "fathurdev",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ez-file-upload.uploadFiles",
        "title": "Upload Files Here..."
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "ez-file-upload.uploadFiles",
          "when": "explorerResourceIsFolder",
          "group": "7_modification"
        }
      ],
      "commandPalette": [
        {
          "command": "ez-file-upload.uploadFiles",
          "when": "workspaceFolderCount > 0"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "release:patch": "npm version patch && git push --follow-tags"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.85.0",
    "@vscode/vsce": "^3.7.1",
    "typescript": "^5.3.0"
  }
}
